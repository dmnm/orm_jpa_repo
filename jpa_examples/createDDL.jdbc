CREATE TABLE EMPLOYEE (id NUMBER(19) NOT NULL, FIRSTNAME VARCHAR, SECONDNAME VARCHAR, department_id NUMBER(19), PRIMARY KEY (id))
CREATE TABLE app_users (ID NUMBER(19) NOT NULL, first_name VARCHAR, second_name VARCHAR, PRIMARY KEY (ID))
CREATE TABLE project_has_employee (project_id NUMBER(19) NOT NULL, employee_id NUMBER(19) NOT NULL, PRIMARY KEY (project_id, employee_id))
ALTER TABLE EMPLOYEE ADD CONSTRAINT FK_EMPLOYEE_department_id FOREIGN KEY (department_id) REFERENCES DEPARTMENT (ID)
