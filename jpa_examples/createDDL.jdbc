CREATE TABLE company (ID NUMBER(19) NOT NULL, company_home VARCHAR(100), NAME VARCHAR, PRIMARY KEY (ID))
CREATE TABLE PROJECT (id NUMBER(19) NOT NULL, NAME VARCHAR, PRIMARY KEY (id))
